{
  "name": "chaterm",
  "version": "0.8.3",
  "description": "A terminal tool with AI Agent, makes you no need to learn complicated regular expressions, Perl and Python, switches and Linux commands, SQL syntax can easily manage thousands of devices!",
  "main": "./out/main/index.js",
  "author": "chaterm.ai",
  "homepage": "https://chaterm.ai",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --fix",
    "lint:staged": "lint-staged",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --incremental ",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --incremental ",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "check:ffmpeg": "node scripts/verify-ffmpeg.js",
    "check:external-deps": "node scripts/check-external-deps.js",
    "check:release-notes": "node scripts/check-release-notes.js",
    "start": "electron-vite preview",
    "dev": "npm run dev:cn",
    "dev:watch": "cross-env APP_EDITION=cn electron-vite dev --watch --mode development.cn",
    "dev:cn": "cross-env APP_EDITION=cn electron-vite dev --mode development.cn",
    "dev:global": "cross-env APP_EDITION=global electron-vite dev --mode development.global",
    "build": "npm run check:external-deps && npm run check:ffmpeg && electron-vite build",
    "build:cn": "npm run check:external-deps && npm run check:ffmpeg && cross-env APP_EDITION=cn electron-vite build --mode production.cn",
    "build:global": "npm run check:external-deps && npm run check:ffmpeg && cross-env APP_EDITION=global electron-vite build --mode production.global",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack:cn": "npm run check:release-notes && npm run build:cn && electron-builder --dir --config electron-builder.cn.yml",
    "build:unpack:global": "npm run check:release-notes && npm run build:global && electron-builder --dir --config electron-builder.global.yml",
    "build:win:cn": "npm run check:release-notes && npm run build:cn && electron-builder --win --config electron-builder.cn.yml",
    "build:win:global": "npm run check:release-notes && npm run build:global && electron-builder --win --config electron-builder.global.yml",
    "build:mac:cn": "npm run check:release-notes && npm run build:cn && electron-builder --mac --config electron-builder.cn.yml",
    "build:mac:global": "npm run check:release-notes && npm run build:global && electron-builder --mac --config electron-builder.global.yml",
    "build:linux:cn": "npm run check:release-notes && node scripts/fix-fsevents-linux.js --linux && npm run build:cn && electron-builder --linux --config electron-builder.cn.yml",
    "build:linux:global": "npm run check:release-notes && node scripts/fix-fsevents-linux.js --linux && npm run build:global && electron-builder --linux --config electron-builder.global.yml",
    "prepare": "husky",
    "test": "vitest",
    "test:main": "vitest --project=main-process",
    "test:renderer": "vitest --project=renderer-process",
    "test:browser": "vitest --project=renderer-browser",
    "test:unit": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --run --coverage --project=main-process --project=renderer-process",
    "test:coverage:main": "vitest --project=main-process --coverage",
    "test:coverage:renderer": "vitest --project=renderer-process --coverage",
    "test:e2e": "playwright test --config playwright.config.ts",
    "test:e2e:ui": "playwright test --config playwright.config.ts --ui",
    "test:e2e:headed": "playwright test --config playwright.config.ts --headed"
  },
  "lint-staged": {
    "*.{js,jsx,.cjs,mjs,ts,tsx,cts,mts,vue}": [
      "prettier --write ",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@anthropic-ai/bedrock-sdk": "^0.26.2",
    "@anthropic-ai/sdk": "^0.72.1",
    "@aws-crypto/client-node": "^4.2.1",
    "@aws-sdk/client-bedrock-runtime": "^3.983.0",
    "@aws-sdk/node-http-handler": "^3.374.0",
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^4.0.0",
    "@kubernetes/client-node": "^1.4.0",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@types/ssh2": "^1.15.5",
    "@vueuse/core": "^14.2.0",
    "adm-zip": "^0.5.16",
    "axios": "^1.13.4",
    "better-sqlite3": "^12.6.2",
    "chokidar": "^5.0.0",
    "clone-deep": "^4.0.1",
    "crypto-js": "^4.2.0",
    "diff": "^8.0.3",
    "dockview-core": "^4.13.1",
    "dockview-vue": "^4.11.0",
    "dotenv": "^17.2.3",
    "electron-updater": "^6.7.3",
    "execa": "^9.6.1",
    "fast-deep-equal": "^3.1.3",
    "fast-json-patch": "^3.1.1",
    "globby": "^16.1.0",
    "memfs": "^4.56.10",
    "monaco-editor": "^0.55.1",
    "node-pty": "^1.0.0",
    "node-vibrant": "^4.0.4",
    "ollama": "^0.6.3",
    "openai": "^6.17.0",
    "p-wait-for": "^6.0.0",
    "pinia": "^3.0.1",
    "pinia-plugin-persistedstate": "^4.2.0",
    "posthog-node": "^5.24.10",
    "serialize-error": "^13.0.1",
    "splitpanes": "^4.0.3",
    "ssh2": "^1.16.0",
    "strip-ansi": "^7.1.0",
    "strip-ansi-cjs": "npm:strip-ansi@^6.0.1",
    "superjson": "^2.2.5",
    "v-contextmenu": "^3.2.0",
    "vue-i18n": "^11.2.8",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-search": "^0.13.0",
    "zmodem.js": "^0.1.10",
    "zod": "^3.24.2",
    "zxcvbn": "^4.4.2"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-typescript": "^7.27.1",
    "@electron-toolkit/eslint-config": "^2.1.0",
    "@electron-toolkit/eslint-config-ts": "^3.1.0",
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.58.1",
    "@rushstack/eslint-patch": "^1.7.1",
    "@types/clone-deep": "^4.0.4",
    "@types/node": "^25.2.0",
    "@types/ws": "^8.5.11",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vitest/browser": "^4.0.18",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.15",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "ant-design-vue": "^4.2.6",
    "cross-env": "^10.1.0",
    "electron": "^40.1.0",
    "electron-builder": "^26.7.0",
    "electron-vite": "^5.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.7.0",
    "highlight.js": "^11.11.1",
    "husky": "^9.1.7",
    "jsdom": "^28.0.0",
    "less": "^4.2.2",
    "lint-staged": "^16.2.7",
    "marked": "^17.0.1",
    "prettier": "^3.8.1",
    "typescript": "^5.8.3",
    "unplugin-auto-import": "^21.0.0",
    "unplugin-vue-components": "^31.0.0",
    "uuid": "^9.0.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.16",
    "vitest-browser-vue": "^2.0.2",
    "vue": "^3.5.27",
    "vue-eslint-parser": "^10.1.3",
    "vue-router": "^5.0.2",
    "vue-tsc": "^3.2.4"
  }
}
